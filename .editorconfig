# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
insert_final_newline = true
indent_style = space
end_of_line = lf
indent_size = 2

# Set default charset
[*.{js,ts}]
charset = utf-8

# Tab indentation (no size specified)
[Makefile]
indent_style = tab